<%content_for :title do%>
	Fasterm - Parceiros
<%end%>

<%content_for :header do%>
	<!-- HEADER -->
	<div id="HeaderInternas">
		<%= image_tag("titulos/conheca_fasterm.png", :alt => "Parceiros")%>
	</div>
<%end%>

		<div id="Content" class="zeraMagem">
			<%= render :file => "layouts/_right.html.erb" %>
			
			<div id="HomeLeft">
				<!-- CONTEUDO INTERNA -->
				<div id="ContentInterno">
					<span class="Titulo"><%= image_tag("bullet_mais.png")%> Login</span><br>
				
					<div id="Texto">
							<%=form_tag sessions_path, :method => :post%>
							<label>
								<p>E-Mail:</p> 
								<input type="Text" value="" name="email" id="email" class="form" style="width:350px;">
							</label>
							<br><br>
							<label>
								<p>Senha: </p>
								<input type="Password" value="" name="senha" id="senha" class="form" style="width:350px;">
							</label>
							<br>
							<br>
							<button type="submit">Logar</button>
					</div>
					
					
				</div>
			</div>
		</div>